                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="59*"/>
                                        <ColumnDefinition Width="56*"/>
                                        <ColumnDefinition Width="33*"/>
                                    </Grid.ColumnDefinitions>
                                    <CheckBox x:Name="chbDisplayCandidate"
                                              Content="{Binding Resources.chbDisplayCandidate, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"                                              
                                              Foreground="White" Margin="20,-15,0,0" Background="White"  IsChecked="True" HorizontalAlignment="Left" Checked="chbAnalyze00_Checked" Unchecked="chbAnalyze00_Checked" VerticalAlignment="Center" FontFamily="MS Gothic" Grid.ColumnSpan="3" VerticalContentAlignment="Center" />
                                    <CheckBox x:Name="chbConfirmMultipleCells"
                                              Content="{Binding Resources.chbConfirmMultipleCells, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"                                              
                                              Background="White" Foreground="White" Margin="20,25,0,-40" IsChecked="True" VerticalAlignment="Top" HorizontalAlignment="Left" FontFamily="MS Gothic" VerticalContentAlignment="Bottom" Grid.ColumnSpan="3" Visibility="Hidden" />
                                    <CheckBox x:Name="chbDifficultySetting"
                                              Content="{Binding Resources.chbDifficultySetting, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"                                              
                                              Background="White" Foreground="White" Margin="20,5,-2,-20"  IsThreeState="False" HorizontalAlignment="Left" IsChecked="True" FontFamily="MS Gothic" Grid.ColumnSpan="3" RenderTransformOrigin="0.549,-0.667" VerticalContentAlignment="Center" Width="130" />
                                </Grid>
                                <Label x:Name="lblAppliedCount"
                                       Content="{Binding Resources.lblAppliedCount, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"                                              
                                       FontSize="12" Foreground="White" Height="23" HorizontalAlignment="Right" Margin="0,134,151,0" VerticalAlignment="Top" Grid.Column="1" Background="#00000000" OpacityMask="White" />

                            </Grid>
                        </TabItem>
