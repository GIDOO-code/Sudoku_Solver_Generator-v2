                     FontSize="11" FontFamily="Microsoft NeoGothic" Height="22" VerticalAlignment="Top" Width="60" >
                <Grid Margin="0,0,0,0" Width="568" >
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <Grid.Background>
                        <LinearGradientBrush EndPoint="1,0.5" StartPoint="0,0.5">
                            <GradientStop Color="#FF3A3948" Offset="0" />
                            <GradientStop Color="#FF65646F" Offset="1" />
                        </LinearGradientBrush>
                    </Grid.Background>
                    <Button x:Name="btnOpenPuzzleFile"
                            Content="{Binding Resources.btnOpenPuzzleFile, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"
                            Width="100" Height="25" VerticalAlignment="Top" Margin="42,31,0,0"
                		Foreground="White" HorizontalAlignment="Left" FontSize="12" Click="btnOpenPuzzleFile_Click" >
                        <Button.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FFA3A3A3" Offset="0" />
                                <GradientStop Color="Black" Offset="1" />
                            </LinearGradientBrush>
                        </Button.Background>
                    </Button>
                    <TextBox Height="39" HorizontalAlignment="Left" Margin="52,61,0,0" x:Name="txtFileName" Text="Puzzle File" VerticalAlignment="Top" Width="299" TextWrapping="WrapWithOverflow" FontSize="12" Background="#FFCACEE2" Foreground="#FF0B0E2B" />
                    <CheckBox x:Name="chbInitialState" 
                              Content="{Binding Resources.chbInitialState, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"
                              Foreground="White" Height="23" IsChecked="True" Margin="57,99,341,0" VerticalAlignment="Top" FontSize="12" VerticalContentAlignment="Center" />
                    <Button x:Name="btnSavePuzzle" Click="btnSavePuzzle_Click"
                            Content="{Binding Resources.btnSavePuzzle, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"
                            Foreground="White" Height="25" HorizontalAlignment="Left" Margin="42,156,0,0" VerticalAlignment="Top" Width="100"  FontSize="12">
                        <Button.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FFB0B0CA" Offset="0" />
                                <GradientStop Color="#FF0D0D1A" Offset="1" />
                                <GradientStop Color="#FF434399" Offset="0.508" />
                            </LinearGradientBrush>
                        </Button.Background>
                    </Button>
                    <Label Content="{Binding Resources.lblSaveAll, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"
                           Foreground="White" Margin="49,181,0,0" Height="28" VerticalAlignment="Top" HorizontalAlignment="Left" Width="94" FontSize="12" />
                    <CheckBox x:Name="chbAdditionalSave" 
                              Content="{Binding Resources.chbAdditionalSave, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"                             
                              Foreground="White" Height="20" Margin="54,200,0,0"  VerticalAlignment="Top" IsChecked="True" HorizontalAlignment="Left" Width="201" FontSize="12" VerticalContentAlignment="Center" />
                    <CheckBox x:Name="chbFile81Nocsv"
                              Content="{Binding Resources.chbFormatLine, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"                             
                              Foreground="White" Margin="54,216,309,0" IsChecked="True" VerticalAlignment="Top" FontSize="12" VerticalContentAlignment="Center" Height="23" Width="185" />
                    <CheckBox x:Name="chbSolutionSort"
                              Content="{Binding Resources.chbSortByDiffOrder, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"                             
                              Foreground="White" Height="16" Margin="54,234,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" Width="176" FontSize="12" VerticalContentAlignment="Center" />
                    <CheckBox x:Name="cbxProbSolSetOutput" 
                              Content="{Binding Resources.chbSaveWithAnswer, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"                  
                              Foreground="White" Margin="54,244,0,0" Height="24" VerticalAlignment="Top" HorizontalAlignment="Left" Width="153" Checked="cbxProbSolSetOutput_Checked" Unchecked="cbxProbSolSetOutput_Checked" FontSize="12" VerticalContentAlignment="Center" />
                    <CheckBox x:Name="chbAddAlgorithmList"
                              Content="{Binding Resources.chbAddAlgorithmList, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"
                              Foreground="Gray" Height="17" HorizontalAlignment="Left" Margin="66,264,0,0" VerticalAlignment="Top" Width="150" IsEnabled="False" FontSize="12" VerticalContentAlignment="Center" />
                    <Button x:Name="btnSaveToFavorites" Click="btnSaveToFavorites_Click" 
                            Content="{Binding Resources.btnSaveToFavorites, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"           
                            Foreground="White" Height="25" HorizontalAlignment="Left" Margin="264,156,0,0"  VerticalAlignment="Top" Width="119" FontSize="12">
                        <Button.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FFCACACA" Offset="0" />
                                <GradientStop Color="#FF6F6F85" Offset="1" />
                                <GradientStop Color="#FF4C31A0" Offset="0.508" />
                            </LinearGradientBrush>
                        </Button.Background>
                    </Button>
                    <Label Content="{Binding Resources.lblSaveCurrentPuzzle, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"           
                           Foreground="White" Height="28" HorizontalAlignment="Left" Margin="272,181,0,0" VerticalAlignment="Top" FontSize="12" Width="138" />
                </Grid>
            </TabItem>

            <TabItem x:Name="tabASolve" 
                     Header="{Binding Resources.tabASolve, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"
                     Style="{DynamicResource ResourceKey=styleTabItemTop}" FontFamily="MS Mincho" FontSize="12">
                <Grid HorizontalAlignment="Left" Width="570">
                    <Grid.Background>
                        <LinearGradientBrush EndPoint="1,0.5" StartPoint="0,0.5">
                            <GradientStop Color="#FF3A3948" Offset="0" />
                            <GradientStop Color="#FF65646F" Offset="1" />
                        </LinearGradientBrush>
                    </Grid.Background>
                    <TabControl Height="396" HorizontalAlignment="Left" Margin="10,6,0,0" x:Name="tabSolver" VerticalAlignment="Top" Width="530"
                                Background="Black" SelectionChanged="tabSolver_SelectionChanged" Grid.ColumnSpan="2">
                        <TabItem x:Name="TC_SDKAnalyzer" 
                                 Header="{Binding Resources.tabBSolve, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"                                              
                                 Style="{DynamicResource ResourceKey=styleTabItemTopSmall}" FontFamily="MS Mincho" FontSize="12">
                            <Grid x:Name="AutoSolNoDisp" Width="526" Margin="0,0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="10*"/>
                                    <ColumnDefinition Width="513*"/>
                                </Grid.ColumnDefinitions>
                                <Grid.Background>
                                    <LinearGradientBrush EndPoint="1,0.5" StartPoint="0,0.5">
                                        <GradientStop Color="#FF3A3948" Offset="0" />
                                        <GradientStop Color="#FF65646F" Offset="1" />
                                    </LinearGradientBrush>
                                </Grid.Background>
                                <Label x:Name="lblStep"
                                       Content="{Binding Resources.lblStep, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"                                              
                                       FontSize="12" Foreground="White" Height="23" HorizontalAlignment="Right" Margin="0,13,62,0" VerticalAlignment="Top" Grid.Column="1" Width="59" HorizontalContentAlignment="Right" />
                                <TextBlock x:Name="txbStepCC" HorizontalAlignment="Right" TextWrapping="Wrap" Text="0" Width="22" Foreground="White" FontFamily="MS Gothic" FontSize="14" TextAlignment="Center" RenderTransformOrigin="0.727,0.917" Margin="0,18,46,336" Grid.Column="1"/>
                                <Label x:Name="LblMethod"
                                       Content="{Binding Resources.LblMethod, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"                                              
                                       FontSize="12" Foreground="White" Height="23" HorizontalAlignment="Left" Margin="99,134,0,0"  VerticalAlignment="Top" Grid.Column="1" Width="46" />
                                <ScrollViewer HorizontalAlignment="Left" Width="366" VerticalScrollBarVisibility="Auto" Grid.Column="1" Margin="0,22,0,232" >
                                    <TextBox Background="#FF0A0A24" Margin="0" x:Name="lblAnalyzerResult" Foreground="#FFF2F2F2" FontFamily="MS Mincho" FontSize="12" HorizontalContentAlignment="Stretch" TextOptions.TextFormattingMode="Display" TextWrapping="WrapWithOverflow" Width="366" HorizontalAlignment="Left" />
                                </ScrollViewer>
                                <ScrollViewer  VerticalScrollBarVisibility="Auto" Grid.Column="1" Margin="1,156,148,5" />

                                <DataGrid AutoGenerateColumns="False" 
                                	x:Name="DGViewMethodCounter" 
                                	Background="#FF0A0A24" 
                                	FontFamily="MS Mincho" 
                                	Foreground="White" 
                                	ItemsSource="{Binding}" 
                                	MinWidth="10" MinHeight="14" 
                                	FontStretch="Normal"
                                	IsSynchronizedWithCurrentItem="True" 
                                	MinRowHeight="14" RowHeight="14" 
                                	CanUserSortColumns="False" 
                                	HeadersVisibility="None" 
                                	MinColumnWidth="12" 
                                	RowBackground="#FF0A0A24" VerticalGridLinesBrush="#FFAFB7EB" 
                                	HorizontalGridLinesBrush="#FFAFB7EB" CanUserResizeColumns="True" CanUserReorderColumns="False" BorderBrush="#FF4F4F72" FrozenColumnCount="2" IsReadOnly="True" SelectionMode="Single" FontSize="12" HorizontalAlignment="Left" Width="364" Grid.IsSharedSizeScope="True" Height="204" Grid.Column="1" Margin="1,154,0,8" >
                                    <DataGrid.Columns>
                                        <DataGridTextColumn FontFamily="MS Gothic" Foreground="White" Binding="{Binding methodName}" Width="5*" />
                                        <DataGridTextColumn FontFamily="MS Gothic" Foreground="White" Binding="{Binding count}" Width="1*"/>
                                    </DataGrid.Columns>
                                    <DataGrid.RowStyle>
                                        <Style TargetType="{x:Type DataGridRow}">
                                            <Style.Triggers>
                                                <Trigger Property="IsSelected" Value="True">
                                                    <Setter Property="Background" Value="#FF0A0A24" />
                                                    <Setter Property="Foreground" Value="White" />
                                                </Trigger>
                                                <Trigger Property="IsFocused" Value="True">
                                                    <Setter Property="Background" Value="#FF0A0A24" />
                                                    <Setter Property="Foreground" Value="White" />
                                                </Trigger>
                                            </Style.Triggers>
                                        </Style>
                                    </DataGrid.RowStyle>
                                </DataGrid>
                                <Button x:Name="btnSolve" 
                                        Content="{Binding Resources.btnSolve, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"                                              
                                        FontSize="12" Foreground="White" 
    								    Margin="389,37,0,0" VerticalAlignment="Top" Width="90" Height="24" HorizontalAlignment="Left"
    								    Click="btnSolve_Click" Grid.Column="1">
                                    <Button.Background>
                                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                            <GradientStop Color="#FFC1B5B5" Offset="0" />
                                            <GradientStop Color="#FF161D6C" Offset="0.472" />
                                            <GradientStop Color="Black" Offset="1" />
                                        </LinearGradientBrush>
                                    </Button.Background>
                                </Button>
                                <Button x:Name="btnUndo"
                                        Content="{Binding Resources.btnUndo, Mode=OneWay, Source={x:Static app:ResourceService.Current}}" 
                                        FontSize="12" Foreground="White" Height="24" HorizontalAlignment="Left" Margin="389,66,0,0" VerticalAlignment="Top" Width="90" Click="btnMPre_Click" Grid.Column="1">
                                    <Button.Background>
                                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                            <GradientStop Color="#FF7EA17E" Offset="0" />
                                            <GradientStop Color="#FF103810" Offset="1" />
                                            <GradientStop Color="#FF184818" Offset="0.626" />
                                        </LinearGradientBrush>
                                    </Button.Background>
                                </Button>
                                <Button x:Name="btnSolveUp" 
                                        Content="{Binding Resources.btnSolveUp, Mode=OneWay, Source={x:Static app:ResourceService.Current}}" 
                                        FontSize="12" Foreground="White" Height="24" 
                                        HorizontalAlignment="Left" Margin="389,110,0,0" VerticalAlignment="Top" 
                                        Width="90" Click="btnSDKAnalyzerAuto_Click" Grid.Column="1">
                                    <Button.Background>
                                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                            <GradientStop Color="#FFC1B5B5" Offset="0" />
                                            <GradientStop Color="#FF161D6C" Offset="0.472" />
                                            <GradientStop Color="Black" Offset="1" />
                                        </LinearGradientBrush>
                                    </Button.Background>
                                </Button>
                                <Button x:Name="btnInitialState"
                                        Content="{Binding Resources.btnInitialState, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"                                        
