<Window x:Name="GNPXwin" x:Class="GNPZ_sdk.NuPz_Win"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:extToolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit/extended" 
        xmlns:my="http://schemas.microsoft.com/wpf/2008/toolkit" 
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"  
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"

        xmlns:properties="clr-namespace:GNPZ_sdk.Properties"
        xmlns:app="clr-namespace:GNPZ_sdk"
     
        xmlns:GIDOO_space="clr-namespace:GIDOO_space"     
        
        mc:Ignorable="d"
        
        Title="NuPz_Window" 
        Loaded="Window_Loaded" Background="#FF181846" ShowInTaskbar="False" WindowStyle="None" 
        ShowActivated="True" ResizeMode="NoResize" Unloaded="Window_Unloaded" BorderBrush="Black" 
        MouseDown="Window_MouseDown" Width="954" Height="513" LocationChanged="GNPXwin_LocationChanged" >
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Styles/DataGridStyle.xaml"></ResourceDictionary>
                <!--            <ResourceDictionary Source="/Styles/ComboBoxStyle.xaml"></ResourceDictionary>   -->
                <ResourceDictionary Source="/Styles/ListBoxStyle1.xaml"></ResourceDictionary>
                <ResourceDictionary Source="/Styles/TabControlStyle.xaml"></ResourceDictionary>
                <ResourceDictionary Source="/Styles/TextBlockStyle.xaml"></ResourceDictionary>

                <ResourceDictionary>
                    <Style TargetType="{x:Type ComboBox}">
                        <Style.Triggers>
                            <Trigger Property="IsKeyboardFocusWithin" Value="true">
                                <Setter Property="Background" Value="Navy"/>
                            </Trigger>
                        </Style.Triggers>
                    </Style>
                </ResourceDictionary>

            </ResourceDictionary.MergedDictionaries>

        </ResourceDictionary>
    </Window.Resources>

    <Grid VerticalAlignment="Stretch" PreviewKeyDown="Grid_PreviewKeyDown">
        <Grid.Background>
            <LinearGradientBrush EndPoint="1,0.5" StartPoint="0,0.5">
                <GradientStop Color="#FF302A3E" />
                <GradientStop Color="#FF5E5E80" Offset="1" />
                <GradientStop Color="#BE232136" Offset="0.43"/>
            </LinearGradientBrush>
        </Grid.Background>
        <Label x:Name="GNPXGNPX" Content="GNPX 2014" FontFamily="MS Mincho" FontSize="36" Foreground="#FFFEFFFF" HorizontalAlignment="Left" 
               Margin="18,10,0,0" VerticalAlignment="Top" Height="41" FontWeight="Bold" MouseDoubleClick="GNPXGNPX_MouseDoubleClick" />
        <Button Content="Exit" FontFamily="MS Gothic" FontSize="12" Foreground="White" Height="29" HorizontalAlignment="Left" Margin="841,474,0,0" x:Name="appExit" VerticalAlignment="Top" Width="56" Click="appExit_Click" Style="{Binding}">
            <Button.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black" Offset="0.8" />
                    <GradientStop Color="#FF7F7E85" Offset="0.05" />
                    <GradientStop Color="#EA575757" Offset="0.55" />
                </LinearGradientBrush>
            </Button.Background>
        </Button>
        <Label Content="LbShortMes" HorizontalAlignment="Left" x:Name="LbShortMes" VerticalAlignment="Top" Foreground="Red" Visibility="Visible" Panel.ZIndex="6" Margin="578,58,0,0" FontFamily="MS Gothic" Background="{x:Null}" />
        <RepeatButton  x:Name="btnProbPre"
                       Content="{Binding Resources.btnProbPre, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"
                       Height="23" HorizontalAlignment="Left" Margin="281,30,0,0"
    		VerticalAlignment="Top" Width="37" Foreground="White" Click="btnProbPre_Click" BorderBrush="#FF2E9BDE" IsEnabled="False" >
            <RepeatButton.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black" Offset="0.8" />
                    <GradientStop Color="#FF7F7E85" Offset="0.05" />
                    <GradientStop Color="#EA575757" Offset="0.55" />
                </LinearGradientBrush>
            </RepeatButton.Background>
        </RepeatButton>
        <RepeatButton x:Name="btnProbNxt"
                      Content="{Binding Resources.btnProbNxt, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"
                      Height="23" HorizontalAlignment="Left" Margin="319,30,0,0" 
    		VerticalAlignment="Top" Width="37" Foreground="White" Click="btnProbNxt_Click" BorderBrush="#FF2E9BDE" IsEnabled="False" >
            <RepeatButton.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black" Offset="0.8" />
                    <GradientStop Color="#FF7F7E85" Offset="0.05" />
                    <GradientStop Color="#EA575757" Offset="0.55" />
                </LinearGradientBrush>
            </RepeatButton.Background>
        </RepeatButton>
        <Label x:Name="lblPuzzle" 
               Content="{Binding Resources.lblPuzzle, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"
               FontFamily="MS Gothic" FontSize="12" Foreground="#FFFEFFFF" HorizontalAlignment="Right" Margin="0,58,892,0" 
               VerticalAlignment="Top"  />
        <TextBox x:Name="txtProbNo" Background="#FF0A0A24" FontFamily="MS Gothic" FontSize="14" Foreground="White" Height="19" HorizontalAlignment="Left" Margin="61,60,0,0" Text="999" TextAlignment="Center" VerticalAlignment="Top" Width="50" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" />
        <TextBox x:Name="txtProbName" Text="........." Background="#FF0A0A24" Width="239" Margin="116,60,0,0" 
                 FontFamily="MS Gothic" FontSize="14" Foreground="White" Height="19" 
                 HorizontalAlignment="Left"   VerticalAlignment="Top" VerticalContentAlignment="Center" 
                 TextChanged="txtProbName_TextChanged" ToolTip="Problem name, optionally rewritable" />

        <!-- DataContext="{Binding}" -->
        <Image x:Name="PB_GBoard" Width="338" Height="338" Margin="18,108,0,0" Stretch="Fill" VerticalAlignment="Top"
                MouseLeftButtonDown="PB_GBoard_MouseLeftButtonDown"
               MouseLeftButtonUp="PB_GBoard_MouseLeftButtonUp" HorizontalAlignment="Left" 
               Source="/GNPXEx;component/10 Graphics/11 WPFColorPickerLib/Images/ColorSwatchCircle.png" />
        <Image Height="36" HorizontalAlignment="Left" Margin="17,107,0,0" x:Name="GnumericPad" Stretch="Fill" 
               VerticalAlignment="Top" Width="36" Visibility="Visible" MouseMove="GnumericPad_MouseMove" 
               MouseUp="GnumericPad_MouseUp" />
        <Label x:Name="lblCellsPuzzle"
            Content="{Binding Resources.lblCellsPuzzle, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"
            FontFamily="MS Gothic" Foreground="White" HorizontalAlignment="Right" Margin="0,87,890,0"  VerticalAlignment="Top" Height="23" Background="{x:Null}" Width="50" HorizontalContentAlignment="Right" FontSize="10" />
        <TextBlock x:Name="txbPuzzle" HorizontalAlignment="Left" Margin="61,91,0,0" TextWrapping="Wrap" Text="00" VerticalAlignment="Top" Width="22" Foreground="#FFC7C5F3" FontFamily="MS Gothic" FontSize="12" TextAlignment="Center" Background="#FF17171B"/>
        <Label x:Name="lblCellsSolved"
            Content="{Binding Resources.lblCellsSolved, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"
            FontFamily="MS Gothic" Foreground="White" HorizontalAlignment="Right" Margin="0,87,817,0"  VerticalAlignment="Top" Height="23" Background="{x:Null}" Width="50" HorizontalContentAlignment="Right" FontSize="10" />
        <TextBlock x:Name="txbSolved" HorizontalAlignment="Left" Margin="131,92,0,0" TextWrapping="Wrap" Text="00" VerticalAlignment="Top" Width="22" Foreground="#FFC7C5F3" FontFamily="MS Gothic" FontSize="12" TextAlignment="Center" Background="#FF17171B"/>
        <Label x:Name="lvlCellsUnsolved"
            Content="{Binding Resources.lvlCellsUnsolved, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"
            FontFamily="MS Gothic" Foreground="White" HorizontalAlignment="Right" Margin="0,87,747,0"  VerticalAlignment="Top" Height="23" Background="{x:Null}" Width="50" HorizontalContentAlignment="Right" FontSize="10" />
        <TextBlock x:Name="txbUnsolved" HorizontalAlignment="Left" Margin="201,92,0,0" TextWrapping="Wrap" Text="00" VerticalAlignment="Top" Width="22" Foreground="#FFC7C5F3" FontFamily="MS Gothic" FontSize="12" TextAlignment="Center" Background="#FF17171B"/>
        <Label x:Name="label1" Margin="0,84,632,0"  VerticalAlignment="Top" Height="23"
               Content="{Binding Resources.lblDifficulty, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"
               FontFamily="MS Gothic" Foreground="White" HorizontalAlignment="Right"  Background="{x:Null}" />
        <TextBox Background="#FF0A0A24" FontFamily="MS Gothic" FontSize="14" Foreground="White" Height="19" HorizontalAlignment="Left" Margin="318,85,0,0" x:Name="nUDDifficultyLevel" Text="0" TextAlignment="Center" VerticalAlignment="Top" Width="37" VerticalContentAlignment="Center" />
        <TabControl x:Name="tabCtrlMode" Background="Black" 
    		Height="445" HorizontalAlignment="Left" Margin="371,23,0,0"
    		VerticalAlignment="Top" Width="573" SelectionChanged="tabCtrlMode_SelectionChanged">
